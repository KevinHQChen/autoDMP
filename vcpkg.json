{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "autodmp",
  "version-string": "0.1.0",
  "builtin-baseline": "a3df696929b28195b0d7a477ba6cd16dba48fa52",
  "dependencies": [
    {
      "name": "fmt",
      "version>=": "8.1.1"
    },
    {
      "name": "cli11",
      "version>=": "2.2.0"
    },
    {
      "name": "spdlog",
      "version>=": "1.10.0"
    },
    {
      "name": "sdl2",
      "version>=": "2.0.20"
    },
    {
      "name": "glew",
      "$feature-description": "OpenGL Extension Wrangler: cross-platform open-source C/C++ extension (anything beyond OpenGL 1.1) library for OpenGL."
    },
    {
      "name": "glfw3",
      "$feature-description": "OpenGL Framework designed for game development"
    },
    {
      "name": "imgui",
      "default-features": false,
      "$feature-description": "Platform/Renderer backends, docking support",
      "features": ["sdl2-binding", "opengl3-binding", "glfw-binding", "docking-experimental"]
    },
    {
      "name": "implot"
    },
    {
      "name": "catch2",
      "version>=": "2.13.8"
    },
    {
      "name": "opencv4",
      "default-features": false,
      "features": ["contrib", "eigen", "ffmpeg", "opengl", "gtk", "jpeg", "png"]
    },
    {
      "name": "boost-program-options"
    },
    {
      "name": "toml11"
    },
    {
      "name": "tsl-ordered-map"
    }
  ]
}
